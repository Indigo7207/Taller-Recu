<?php
$piland = array();

array_push($piland, 60);
array_push($piland, 45);
array_push($piland, 70);
array_push($piland, 30);
array_push($piland, 80);
array_push($piland, 55);
array_push($piland, 20);
array_push($piland, 90);
array_push($piland, 40);
array_push($piland, 65);

foreach ($piland as $numeros) {
    if ($numeros > 50) {
      
      echo $numeros . "\n","<3";
      
     
    }
}
?>
